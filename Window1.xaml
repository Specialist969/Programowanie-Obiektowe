<Window x:Class="ProgramowanieObiektowe.Window1"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProgramowanieObiektowe"
        mc:Ignorable="d"
        Title="Window1" Height="450" Width="800">
    <Grid>
        <DataGrid x:Name="TabelaDuża" HorizontalAlignment="Left" Height="363" Margin="473,34,0,0" VerticalAlignment="Top" Width="292" d:ItemsSource="{d:SampleData ItemCount=5}" SelectionChanged="TabelaDuża_SelectionChanged"/>
        <DataGrid x:Name="TabelaMała" HorizontalAlignment="Left" Height="154" Margin="10,243,0,0" VerticalAlignment="Top" Width="411" d:ItemsSource="{d:SampleData ItemCount=5}" SelectionChanged="TabelaMała_SelectionChanged"/>

    </Grid>
</Window>
